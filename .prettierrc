{
  "singleQuote": true,
  "trailingComma": "all",
  "useTabs": false,
  "tabWidth": 2,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/(.*)$",
    "(.css|.scss)$",
    "^[./]"
  ],
  "importOrderTypeImportsToBottom": true,
  "importOrderSeparation": true,
  "importOrderSortIndividualImports": true,
  "plugins": ["@serverless-guru/prettier-plugin-import-order"]
}
